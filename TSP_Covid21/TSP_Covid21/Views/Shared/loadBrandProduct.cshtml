@model IEnumerable<TSP_Covid21.Models.ShopEntity.TEMPPRODUCT>

<div class="aside">
    <h3 class="aside-title">Brand</h3>
    <div class="checkbox-filter">

        @foreach (var item in Model)
        {
            <div class="input-checkbox">
                <input type="checkbox" id="@item.BRANDID">
                <label for="brand-1">
                    <span></span>
                    @item.BRAND.BRANDNAME
                    <small>@item.BRAND.PRODUCTs.Count()</small>
                </label>
            </div>
        }
    </div>
</div>
